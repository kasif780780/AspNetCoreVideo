@model DashboardViewModel
@{
    ViewData["Title"] = "Dashboard";
}

<h2>Dashboard</h2>


@foreach (var dashboardrow in Model.Courses)
{
    <div class="row">
        @foreach(var course in dashboardrow)
        {
            @await Html.PartialAsync("_CoursePanelPartial",course)

        }
    </div>

}