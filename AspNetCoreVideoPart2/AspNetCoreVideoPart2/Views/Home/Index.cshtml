
@model IEnumerable<AspNetCoreVideoPart2.ViewModels.VideoViewModel>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Video</title>
</head>
<body>
    <table>
        @foreach (var item in Model)
        {
        <tr>
            <td>@Html.ActionLink(item.Id.ToString(), "Details", new { id = item.Id })</td> 
            <td>@item.Title</td>
            <td>
                @item.Genre
            </td>
        </tr>
        }
    </table>
</body>
</html